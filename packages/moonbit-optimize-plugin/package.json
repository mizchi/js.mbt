{
  "name": "moonbit-optimize-plugin",
  "version": "0.1.0",
  "description": "Optimization plugin for MoonBit generated JavaScript code",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "moonbit-optimize": "./scripts/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./vite": {
      "import": "./src/unplugin.ts"
    },
    "./rollup": {
      "import": "./src/unplugin.ts"
    },
    "./webpack": {
      "import": "./src/unplugin.ts"
    },
    "./esbuild": {
      "import": "./src/unplugin.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "test": "node --test tests/**/*.test.ts"
  },
  "keywords": [
    "moonbit",
    "optimization",
    "ffi",
    "inline",
    "unplugin",
    "vite",
    "rollup",
    "webpack",
    "esbuild"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.14.0",
    "astring": "^1.9.0",
    "magic-string": "^0.30.18",
    "oxc-minify": "^0.46.0",
    "unplugin": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "vite": "^7.2.6"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
